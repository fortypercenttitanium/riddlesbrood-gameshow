{
	"main": "public/electron.js",
	"name": "riddlesbrood-gameshow",
	"version": "0.1.1",
	"description": "Gameshow presentation app for Riddlesbrood Theatre Company",
	"author": "ayWebDev",
	"private": true,
	"build": {
		"appId": "riddlesbrood.gameshow",
		"productName": "Riddlesbrood Gameshow",
		"copyright": "Copyright 2020 Riddlesbrood Theatre Company",
		"win": {
			"icon": "./public/media/images/icon.png"
		}
	},
	"homepage": "./",
	"dependencies": {
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material/slider": "^7.0.0",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"cross-env": "^7.0.2",
		"electron-is-dev": "^1.2.0",
		"electron-store": "^5.2.0",
		"react": "^16.13.1",
		"react-audio-player": "^0.13.0",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^3.4.3",
		"styled-components": "^5.1.1"
	},
	"resolutions": {
		"styled-components": "^5"
	},
	"scripts": {
		"react-start": "react-scripts start",
		"react-build": "react-scripts build",
		"react-test": "react-scripts test",
		"react-eject": "react-scripts eject",
		"electron-build": "electron-builder",
		"electron-dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
		"electron-pack": "electron-builder electron-builder -c.extraMetadata.main=build/electron.js",
		"preelectron-pack": "npm run react-build"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^5.2.0",
		"electron": "^9.0.5",
		"electron-builder": "^22.7.0",
		"electron-react-devtools": "^0.5.3",
		"wait-on": "^5.0.1"
	}
}
